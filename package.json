{
  "name": "url-query-purifier",
  "displayName": "Url query purifier",
  "version": "0.0.1",
  "description": "URL query string remover",
  "author": "zukamiso",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.89.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "description": "Removes '&list=WL' from YouTube URLs before loading.",
    "permissions": ["declarativeNetRequest"],
    "host_permissions": ["*://www.youtube.com/*"]
  }
}
